public class CarroAtributos {
	
	private String motorCarro;
	private String corCarro;
	private String rodasCarro;
	private int potenciaCarro;
	private float velocidadeCarro;
	
	
	public float getVelocidadeCarro() {
		return velocidadeCarro;
	}
	public void setVelocidadeCarro(float velocidadeCarro) {
		this.velocidadeCarro = velocidadeCarro;
	}
	public int getPotenciaCarro() {
		return potenciaCarro;
	}
	public void setPotenciaCarro(int potenciaCarro) {
		this.potenciaCarro = potenciaCarro;
	}
	public String getRodasCarro() {
		return rodasCarro;
	}
	public void setRodasCarro(String rodasCarro) {
		this.rodasCarro = rodasCarro;
	}
	public String getCorCarro() {
		return corCarro;
	}
	public void setCorCarro(String corCarro) {
		this.corCarro = corCarro;
	}
	public String getMotorCarro() {
		return motorCarro;
	}
	public void setMotorCarro(String motorCarro) {
		this.motorCarro = motorCarro;
	}

}
